<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/profile_layout" android:background="#ffffff"
    >

    <RelativeLayout
        android:id="@+id/profile_menu_bar_container"
        android:layout_width="match_parent"
        android:layout_height="51dp"
        android:background="#ffffff">

        <RelativeLayout
            android:id="@+id/profile_cancel_container"
            android:layout_width="51dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:clickable="true">

            <ImageView
                android:id="@+id/profile_btn_cancel"
                android:layout_width="6.33dp"
                android:layout_height="12.33dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="16.33dp"
                android:background="@null"
                android:scaleType="fitXY"
                android:src="@drawable/upload_btn_back"

                />

        </RelativeLayout>

        <TextView
            android:id="@+id/profile_txt_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/profile_cancel_container"
            android:text="@string/profile_title"
            android:textSize="17dp"
            android:textStyle="bold" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingRight="16.33dp">

            <Button
                android:id="@+id/profile_btn_edit"
                android:layout_width="42dp"
                android:layout_height="34.33dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="#f7c243"
                android:text="@string/profile_btn_modify"
                android:textSize="9dp"
                android:textColor="#ffffff" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0.33dp"
            android:layout_alignParentBottom="true"
            android:background="#000000">

        </RelativeLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/profile_user_profile_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/profile_menu_bar_container">

        <RelativeLayout
            android:id="@+id/profile_guide_container"
            android:layout_width="match_parent"
            android:layout_height="40.66dp"
            android:background="#ececec">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/profile_banner_modify_profile"
                android:textSize="12dp" />

            <RelativeLayout
                android:id="@+id/profile_guide_cancel_container"
                android:layout_width="51dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:clickable="true">

                <ImageButton
                    android:id="@+id/profile_btn_guide_cancel"
                    android:layout_width="12dp"
                    android:layout_height="12.33dp"
                    android:layout_centerInParent="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="16.33dp"
                    android:background="@null"
                    android:scaleType="fitXY"
                    android:src="@drawable/more_btn_02_cancel"

                    />

            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/profile_user_photo_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/profile_guide_container"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="21dp">

            <ImageView
                android:id="@+id/profile_img_user_photo_main"
                android:layout_width="97.66dp"
                android:layout_height="97.66dp"
                android:layout_centerInParent="true"
                android:src="@drawable/more_img_06_01_cat"
                />

            <ImageView
                android:id="@+id/profile_btn_camera"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignBottom="@id/profile_img_user_photo_main"
                android:layout_alignRight="@id/profile_img_user_photo_main"
                android:layout_marginRight="5.33dp"
                android:src="@drawable/mypage_profile_btn_photo"
                android:scaleType="fitXY"
                android:background="@null"

                />

         </RelativeLayout>

        <RelativeLayout
            android:id="@+id/profile_user_name_container"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true"
            android:layout_below="@id/profile_user_photo_container"
            >

            <ImageView
                android:id="@+id/profile_img_user_clan"
                android:layout_width="19dp"
                android:layout_height="14dp"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"

                />

            <TextView
                android:id="@+id/profile_txt_user_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="멍멍이"
                android:textSize="16dp"
                android:textColor="#000000"
                android:layout_toRightOf="@id/profile_img_user_clan"
                android:layout_marginLeft="5dp"
                android:layout_centerVertical="true"
                />

        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/profile_input_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/profile_user_profile_container"
        android:layout_marginLeft="19.66dp"
        android:layout_marginRight="19.66dp"
        android:layout_marginTop="21dp"
        >

        <RelativeLayout
            android:id="@+id/profile_input_gender"
            android:layout_width="match_parent"
            android:layout_height="83.33dp"
            android:clickable="true"
            >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                >

                <ImageView
                    android:id="@+id/profile_img_gender"
                    android:layout_width="12.67dp"
                    android:layout_height="12.33dp"
                    android:src="@drawable/profile_icon_01"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="3dp"
                    />

                <TextView
                    android:id="@+id/profile_txt_gender_state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14.5dp"
                    android:textColor="#494949"
                    android:text="@string/profile_txt_gender"
                    android:layout_toRightOf="@id/profile_img_gender"

                    />

            </RelativeLayout>



            <RelativeLayout
                android:id="@+id/profile_gender_disable_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="10dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/profile_edit_gender"
                    android:textColor="#b8b8b8"
                    />

                <ImageButton
                    android:id="@+id/profile_btn_gender"
                    android:layout_width="12dp"
                    android:layout_height="6.33dp"
                    android:src="@drawable/more_btn_03_arrow"
                    android:layout_alignParentRight="true"
                    android:scaleType="fitXY"
                    android:background="@null"
                    android:layout_centerVertical="true"
                    android:visibility="invisible"
                    />

           </RelativeLayout>

            <TextView
                android:id="@+id/profile_txt_gender"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:textColor="#000000"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="10dp"
                android:layout_alignParentLeft="true"
                android:textSize="17dp"
                />


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0.33dp"
                android:background="#f7c243"
                android:layout_alignParentBottom="true"
                >

            </RelativeLayout>


            <LinearLayout
                android:id="@+id/profile_select_gender_container"
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:orientation="horizontal"
                android:layout_alignParentBottom="true"
                android:visibility="invisible"
                >

                <RelativeLayout
                android:id="@+id/profile_gender_man_container"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                >
                <TextView
                    android:id="@+id/profile_txt_gender_man"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:textColor="#494949"
                    android:text="@string/profile_edit_gender_man"
                    />

            </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/profile_gender_uni_container"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"

                    >
                    <TextView
                        android:id="@+id/profile_txt_gender_uni"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:textColor="#494949"
                        android:text="@string/profile_edit_gender_uni"
                        />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/profile_gender_woman_container"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"

                    >
                    <TextView
                        android:id="@+id/profile_txt_gender_woman"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:textColor="#494949"
                        android:text="@string/profile_edit_gender_woman"
                        />

                </RelativeLayout>

            </LinearLayout>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/profile_input_birth"
            android:layout_width="match_parent"
            android:layout_height="83.33dp"
            android:layout_below="@id/profile_input_gender"
            >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="15dp"
                    >

                    <ImageView
                        android:id="@+id/profile_img_birth"
                        android:layout_width="13dp"
                        android:layout_height="13.67dp"
                        android:src="@drawable/profile_icon_02"
                        android:scaleType="fitXY"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="3dp"
                        />

                    <TextView
                        android:id="@+id/profile_txt_birth_state"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14.5dp"
                        android:textColor="#494949"
                        android:text="@string/profile_txt_birthday"
                        android:layout_toRightOf="@id/profile_img_birth"
                        android:layout_centerVertical="true"

                        />

                </RelativeLayout>


                <TextView
                    android:id="@+id/profile_txt_choice_birth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/profile_edit_birthday"
                    android:textColor="#b8b8b8"
                    android:layout_alignParentBottom="true"
                    />

                <TextView
                    android:id="@+id/profile_txt_set_birth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    android:visibility="invisible"
                    android:textSize="17dp"
                    android:textColor="#000000"
                    />

                <ImageButton
                    android:id="@+id/profile_btn_birth"
                    android:layout_width="12dp"
                    android:layout_height="6.33dp"
                    android:src="@drawable/more_btn_03_arrow"
                    android:layout_alignParentRight="true"
                    android:scaleType="fitXY"
                    android:background="@null"
                    android:layout_centerVertical="true"
                    android:visibility="invisible"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0.33dp"
                android:background="#f7c243"
                android:layout_alignParentBottom="true"
                />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/profile_input_state"
            android:layout_width="match_parent"
            android:layout_height="83.33dp"
            android:layout_below="@id/profile_input_birth"
            >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0.33dp"
                android:background="#f7c243"
                android:layout_alignParentBottom="true"
                />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="15dp"
                    >

                    <ImageView
                        android:id="@+id/profile_img_status"
                        android:layout_width="13.33dp"
                        android:layout_height="11.33dp"
                        android:src="@drawable/profile_icon_03"
                        android:scaleType="fitXY"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="3dp"
                        />

                    <TextView
                        android:id="@+id/profile_txt_status_state"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14.5dp"
                        android:textColor="#494949"
                        android:text="@string/profile_txt_status_message"
                        android:layout_toRightOf="@id/profile_img_status"
                        android:layout_centerVertical="true"

                        />

                </RelativeLayout>

                <TextView
                    android:id="@+id/profile_txt_choice_state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="* 상태를 선택 하라냥"
                    android:textColor="#b8b8b8"
                    android:layout_alignParentBottom="true"
                    />

                <TextView
                    android:id="@+id/profile_txt_set_state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    android:textSize="17dp"
                    android:textColor="#000000"
                    />

                <ImageButton
                    android:id="@+id/profile_btn_state"
                    android:layout_width="12dp"
                    android:layout_height="6.33dp"
                    android:src="@drawable/more_btn_03_arrow"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:scaleType="fitXY"
                    android:background="@null"
                    android:visibility="invisible"
                    />

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/profile_overlay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#70000000"
        android:visibility="invisible">

    </RelativeLayout>


</RelativeLayout>